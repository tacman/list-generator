{
    "name": "umpirsky/list-generator",
    "type": "library",
    "description": "Export data to various formats.",
    "license": "MIT",
    "authors": [
        {
            "name": "Saša Stamenković",
            "email": "umpirsky@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "symfony/console": "^2.7|^3.0",
        "symfony/filesystem": "^2.7|^3.0",
        "symfony/finder": "^2.7|^3.0",
        "symfony/yaml": "^2.7|^3.0",
        "doctrine/dbal": "~2.5",
        "zendframework/zend-db": "^2.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-0": {
            "Umpirsky\\ListGenerator": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Umpirsky\\ListGenerator\\": "tests/"
        }
    }
}
